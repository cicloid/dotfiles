                            "   call Decho("looking for trailing nonisi<".nonisi."> followed by a j, gj, or jg",'~'.expand("<slnum>"))
                               if pat =~# nonisi.'j$\|'.nonisi.'gj$\|'.nonisi.'jg$'
                                call s:NetrwMarkFileQFEL(a:islocal,getqflist())
                               endif
                              endif
                            
