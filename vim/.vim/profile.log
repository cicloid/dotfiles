                            "                     Uses the global markfilelist
    1              0.000001 fun! s:NetrwMarkFileGrep(islocal)
                            "  call Dfunc("s:NetrwMarkFileGrep(islocal=".a:islocal.")")
                              let svpos    = winsaveview()
                            "  call Decho("saving posn to svpos<".string(svpos).">",'~'.expand("<slnum>"))
