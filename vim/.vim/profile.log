                                 elseif g:netrw_browse_split == 2
                                  " vertically splitting the window first
                            "      call Decho("edit-a-file: vertically splitting window prior to edit",'~'.expand("<slnum>"))
                                  keepalt rightb vert new
                                  if !&ea
                                   keepalt wincmd |
                                  endif
                                  call s:SetRexDir(a:islocal,curdir)
