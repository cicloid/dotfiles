                              " NetWrite: Get Temporary Filename {{{3
                              let tmpfile= s:GetTempfile("")
                              if tmpfile == ""
                            "   call Dret("netrw#NetWrite : unable to get a tempfile!")
                               return
                              endif
                            
                              if a:0 == 0
                               let ichoice = 0
                              else
                               let ichoice = 1
                              endif
                            
                              let curbufname= expand("%")
                            "  call Decho("curbufname<".curbufname.">",'~'.expand("<slnum>"))
