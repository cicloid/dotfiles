                               let ret= fname
                            "   call Decho("vim and netrw agree on current directory (g:netrw_keepdir=".g:netrw_keepdir.")",'~'.expand("<slnum>"))
                            "   call Decho("vim   directory: ".getcwd(),'~'.expand("<slnum>"))
