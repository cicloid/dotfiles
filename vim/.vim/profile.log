                               call s:NetrwUnmarkAll()
                              else
                            "   call Decho('no marked files, using "*"','~'.expand("<slnum>"))
                               let netrwmarkfilelist= "*"
                              endif
                            
                              " ask user for pattern
                              call inputsave()
                              let pat= input("Enter pattern: ","")
                              call inputrestore()
                              let patbang = ""
