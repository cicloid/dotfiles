                            " ------------------------------------------------------------------------
                            " netrw#NetWrite: responsible for writing a file over the net {{{2
    1              0.000003 fun! netrw#NetWrite(...) range
                            "  call Dfunc("netrw#NetWrite(a:0=".a:0.") ".g:loaded_netrw)
                            
                              " NetWrite: option handling {{{3
                              let mod= 0
                              call s:NetrwOptionSave("w:")
